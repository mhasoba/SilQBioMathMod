# param3d.ode
# 3d parametric plots
f(t)=cos(a*t)
g(t)=sin(b*t)
h(t)=sin(c*t+d)
par a=1.34,b=1.12,c=2.28,d=.75
par zlo=-4
s'=1
aux x=f(s)
aux y=g(s)
aux z=h(s)
aux zplane=zlo
@ xp=x,yp=y,zp=z,xlo=-2,ylo=-2,xhi=2,yhi=2
@ axes=3d
@ xmax=1.25,ymax=1.25,zmax=1.25,xmin=-1.25,ymin=-1.25,zmin=-4
@ nplot=2,xp2=x,yp2=y,zp2=zplane
@ total=100
done








