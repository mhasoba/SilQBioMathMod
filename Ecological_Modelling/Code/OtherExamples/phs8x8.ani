# phs8x8.ani
# use with phs8x8.ode
frect 0/8;[0..7]/8;1/8;[j+1]/8;x1_[j+1]/(2*pi)
frect 1/8;[0..7]/8;2/8;[j+1]/8;x2_[j+1]/(2*pi)
frect 2/8;[0..7]/8;3/8;[j+1]/8;x3_[j+1]/(2*pi)
frect 3/8;[0..7]/8;4/8;[j+1]/8;x4_[j+1]/(2*pi)
frect 4/8;[0..7]/8;5/8;[j+1]/8;x5_[j+1]/(2*pi)
frect 5/8;[0..7]/8;6/8;[j+1]/8;x6_[j+1]/(2*pi)
frect 6/8;[0..7]/8;7/8;[j+1]/8;x7_[j+1]/(2*pi)
frect 7/8;[0..7]/8;8/8;[j+1]/8;x8_[j+1]/(2*pi)
end